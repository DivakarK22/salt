{
    "checks": {
       "redis": {
         "command": "/etc/sensu/conf.d/checks/check-process.rb -p redis",
         "subscribers": [
           "dev"
         ],
         "interval": 10
       },
       "chronyd": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p chronyd",
         "subscribers": [
           "dev"
         ],
         "interval": 10
       },
       "rabbitmq-server": {
         "command": "/etc/sensu/conf.d/checks/check-process.rb -p rabbitmq-server",
         "subscribers": [
           "dev"
         ],
         "interval": 10
       },
       "sensu-server": {
         "command": "/etc/sensu/conf.d/checks/check-process.rb -p sensu-server -c 0",
         "subscribers": [
           "dev"
         ],
         "interval": 1
       },
       "kdump": {
         "command": "/etc/sensu/conf.d/checks/check-process.rb -p kdump",
         "subscribers": [
           "dev"
         ],
         "interval": 10
       },
       "sshd": {
         "command": "/etc/sensu/conf.d/checks/check-process.rb -p sshd",
         "subscribers": [
           "sensu",
           "dev"
         ],
         "interval": 10
       },
       "check-cpu": {
         "command": "/etc/sensu/conf.d/checks/check-process.rb -c 10",
         "subscribers": [
           "dev",
           "test"
         ],
         "interval": 10
       }
  }

}
