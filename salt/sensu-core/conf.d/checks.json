{
    "checks": {
       "redis": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p redis",
         "subscribers": [
           "sensu-master"
         ],
         "interval": 10
       },
       "httpd": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p httpd",
         "subscribers": [
           "client:ftp"
         ],
         "interval": 10
       },
       "salt-minion": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p salt-minion",
         "subscribers": [
           "el7"
         ],
         "interval": 10
       },
       "sshd": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p sshd",
         "subscribers": [
           "sensu-master",
           "el7"
         ],
         "interval": 10
       },
       "check-cpu": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -c 155",
         "subscribers": [
           "sensu-master",
           "el7"
         ],
         "interval": 10
       },
       "check-memory": {
        "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-memory.rb  -w 100 -c 50",
        "subscribers": [
          "el7"
        ],
        "interval": 10
      },
      "jenkins-http": {
        "command": "/etc/sensu/conf.d/checks/check-http.rb -u http://jenkins:8080 -U devops -a devops",
        "subscribers": [
          "client:jenkins"
        ],
        "interval": 10
      },
      "grafana-http": {
        "command": "/etc/sensu/conf.d/checks/check-http.rb -u http://grafana:3000 -U admin -a devops",
        "subscribers": [
          "client:grafana"
        ],
        "interval": 10
      },
      "node-exporter": {
        "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p node_exporter",
        "subscribers": [
          "el7"
        ],
        "interval": 10
      },
      "prometheus": {
        "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p prometheus",
        "subscribers": [
          "client:grafana"
        ],
        "interval": 10
      },
      "lm-sensor": {
        "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p lm_sensors",
        "subscribers": [
          "el7"
        ],
        "interval": 10
      }


      
  }

}
