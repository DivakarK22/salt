{
    "checks": {
       "redis": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p redis",
         "subscribers": [
           "sensu-master"
         ],
         "interval": 10
       },
       "httpd": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p httpd",
         "subscribers": [
           "client:ftp"
         ],
         "interval": 10
       },
       "sensu-server": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p sensu-server",
         "subscribers": [
           "sensu-master"
         ],
         "interval": 10
       },
       "salt-minion": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p salt-minion",
         "subscribers": [
           "el7"
         ],
         "interval": 10
       },
       "sshd": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -p sshd",
         "subscribers": [
           "sensu-master",
           "el7"
         ],
         "interval": 10
       },
       "check-cpu": {
         "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-process.rb -c 140",
         "subscribers": [
           "sensu-master",
           "el7"
         ],
         "interval": 10
       }
       "check-memory": {
        "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-memory.rb  -w 250 -c 100",
        "subscribers": [
          "el7"
        ],
        "interval": 10
      }
      "check-disk": {
        "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-disk-usage.rb -c 85 -w 75",
        "subscribers": [
          "el7"
        ],
        "interval": 10
      }
      "check-hosts": {
        "command": "/opt/sensu/embedded/bin/ruby /etc/sensu/conf.d/checks/check-cmd.rb -o 64  -c "ping -c 2 jenkins;ping -c 2 grafana;ping -c 2 sensu-core;ping -c 2 ansible;ping -c 2 k8;ping -c 2 sensu;ping -c 2 ftp;ping -c 2 docker",
        "subscribers": [
          "el7"
        ],
        "interval": 10
      }
  }

}
